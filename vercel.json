{
    "version": 2,
    "builds": [
        {
            "src": "./server/server.js",
            "use": "@now/node"
        },
        {
            "src": "./client/build",
            "use": "@now/static"
        }
    ],

    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/server/server.js"
        }
    ]
}
